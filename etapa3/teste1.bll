// Declarações globais são permitidas
int a;
bool controle;

// Código executável deve ir dentro de uma função
int main() {
    a = 10; 

    controle = true;
    while (controle) {
        a = a + 1;
        if (a == 15) {
            controle = false;
            if(a < 10){
                controle = true;
            }else{
                controle = false;
            }
        }else if(true){
            controle = true;
        }
    }
    print(a);
}