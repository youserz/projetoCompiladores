// Declarações globais são permitidas
int a;
int b;
bool controle;

// Código executável deve ir dentro de uma função
int main() {
    a = 10; 
    b = 20;

    if (a < b) {
        print(a);
    } else {
        print(b);
    }
    controle = true;
    while (controle) {
        a = a + 1;
        if (a == 15) {
            controle = false;
        }else{
            controle = true;
        }
    }
    print(a);
}